<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"

    android:background="@color/black"
    android:orientation="horizontal">

    <RelativeLayout
        android:id="@+id/rl_timing_handno"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_weight="1"
        android:background="#ffffff">

        <TextView
            android:id="@+id/tv_timeResultHandNo"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_centerInParent="true"
            android:background="@drawable/shape_round_textview"
            android:gravity="center"
            android:text="1" />

    </RelativeLayout>

    <TextView
        android:id="@+id/tv_timeResultStudentName"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginLeft="0.5dp"
        android:layout_weight="2"
        android:background="#ffffff"
        android:gravity="center"
        android:text="欧阳欧阳" />

    <TextView
        android:id="@+id/tv_timeResult"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginLeft="0.5dp"
        android:layout_weight="2"
        android:background="#ffffff"
        android:gravity="center"
        android:text="1" />

    <!--<TextView-->
    <!--android:id="@+id/tv_timeResultFlag"-->
